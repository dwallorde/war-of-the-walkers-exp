                        <label depth="3" pos="9, -6" text="{entrydescription}"  width="530" height="333" font_size="26" parse_actions="true" actions_default_format="( [e61a16]###[-] )" />
                    </rect>
                    
                    <rect name="ChallengeBox1" depth="1" pos="0,-383" width="553" height="376" visible="{# objectivecount == 1}" >
                        <sprite depth="1" name="background" color="0,0,0,145" type="sliced"/>
