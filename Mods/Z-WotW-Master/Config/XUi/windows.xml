	<append xpath="/windows">
        <window name="windowChallengeEntryDescription" width="553" height="600" panel="Center" controller="Quartz.ChallengeEntryDescriptionWindow, Quartz" cursor_area="true" >
            <panel name="header" height="43" depth="1" backgroundspritename="ui_game_panel_header">
                <sprite depth="2" name="windowIcon" style="icon32px" pos="4,-5" sprite="ui_game_symbol_pen"/>
                <label depth="2" pos="345,-8" width="100" height="28" text="{timestamp}" color="[lightGrey]" justify="right" upper_case="true" />
                <label style="header.name" text_key="challenge" width="400" />
            </panel>
            <rect name="content" pos="0,-46" height="711" depth="1">
            
                <!-- <sprite depth="6" name="backgroundMain" sprite="menu_empty3px" width="100" height="103" color="[black]" type="sliced" fillcenter="false" />  -->
                <sprite depth="1" pos="2,-2" name="preview" width="97" height="100" color="[darkGrey]" type="sliced" />
                <sprite depth="3" pos="50,-50" width="80" height="80" sprite="{entryicon}" pivot="center" foregroundlayer="true"/>

                <button pos="520,-50" depth="12" name="gotoButton" style="icon32px" sprite="ui_game_symbol_go_to_button" defaultcolor="[white]" pivot="center" collider_scale="1.5" sound="[paging_click]" visible="{showgoto}"/>

                <rect name="descriptionBox" depth="1" pos="97,0" width="456" height="100" >
                    <!-- <sprite depth="1" name="background" color="[mediumGrey]" type="sliced"/> -->
