</append>

<set xpath="/windows/window[@name='ingameMenuLogo']/texture/@texture">@modfolder(WaroftheWalkersMod):Textures\Menu\wwmenulogo.png</set>
<set xpath="/windows/window[@name='ingameMenuLogo']/texture/@anchor_left">#cam,0,120</set>
<set xpath="/windows/window[@name='ingameMenuLogo']/texture/@anchor_bottom">#cam,0.25,150</set>
