                    <label depth="2" name="descriptionText" pos="10,-6" width="538" height="172" font_size="22" text="{entryshortdescription}" parse_actions="true" actions_default_format="( [e61a16]###[-] )"/>
                </rect>

                <grid name="objectives" pos="0,-173" rows="8" width="520" height="520" cell_width="520" cell_height="40" arrangement="vertical">
                
                    <rect depth="2" width="520" height="30" controller="Quartz.ChallengeEntryObjectiveEntry, Quartz" visible="{hasobjective}">
                        <rect pos="15,0">
                            <sprite depth="1" name="border" color="0,0,0,150" height="30" type="sliced" />
                            <sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

                            <filledsprite depth="3" name="background" color="110,110,110,128" type="filled" fill="1" />

                            <filledsprite depth="4" name="BarContent" color="0,128,128,255" type="filled" fill="{objectivefill}" />
                            <label depth="6" name="TextContent" pos="0,-4" font_size="20" color="[white]" justify="center" text="{objective}"/>

                            <sprite depth="8" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>
                            
                        </rect>
                    </rect>
                    
                    <rect depth="2" width="520" height="30" controller="Quartz.ChallengeEntryObjectiveEntry, Quartz" visible="{hasobjective}" >
                        <rect pos="15,0">
                            <sprite depth="1" name="border" color="0,0,0,150" height="30" type="sliced" />
                            <sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

                            <filledsprite depth="3" name="background" color="110,110,110,128" type="filled" fill="1" />

                            <filledsprite depth="4" name="BarContent" color="0,128,128,255" type="filled" fill="{objectivefill}" />
                            <label depth="6" name="TextContent" pos="0,-4" font_size="20" color="[white]" justify="center" text="{objective}"/>

                            <sprite depth="8" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>
                        </rect>
                    </rect>

                    <rect depth="2" width="520" height="30" controller="Quartz.ChallengeEntryObjectiveEntry, Quartz" visible="{hasobjective}" >
                        <rect pos="15,0">
                            <sprite depth="1" name="border" color="0,0,0,150" height="30" type="sliced" />
                            <sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

                            <filledsprite depth="3" name="background" color="110,110,110,128" type="filled" fill="1" />

                            <filledsprite depth="4" name="BarContent" color="0,128,128,255" type="filled" fill="{objectivefill}" />
                            <label depth="6" name="TextContent" pos="0,-4" font_size="20" color="[white]" justify="center" text="{objective}"/>

                            <sprite depth="8" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>
                        </rect>
                    </rect>
					
					<rect depth="2" width="520" height="30" controller="Quartz.ChallengeEntryObjectiveEntry, Quartz" visible="{hasobjective}" >
                        <rect pos="15,0">
                            <sprite depth="1" name="border" color="0,0,0,150" height="30" type="sliced" />
                            <sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

                            <filledsprite depth="3" name="background" color="110,110,110,128" type="filled" fill="1" />

                            <filledsprite depth="4" name="BarContent" color="0,128,128,255" type="filled" fill="{objectivefill}" />
