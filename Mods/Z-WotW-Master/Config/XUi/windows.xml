                    <label depth="2" pos="12,-18" width="538" height="93" text="{entrytitle}"/>
                    <label depth="2" pos="12,-46" width="538" height="93" text="{entrygroup}" font_size="22" color="[disabledLabelColor]" />
                </rect>
                
                <rect name="detailsBox" depth="1" pos="0,-100" width="553" height="179" >
                    <!-- <sprite depth="1" name="background" color="[mediumGrey]" type="sliced"/> -->
