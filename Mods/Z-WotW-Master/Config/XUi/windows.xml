                    </rect>
                
                </grid>
            
                <sprite depth="8" name="backgroundMain" sprite="menu_empty3px" pos="0,0" width="553" height="711" color="[black]" type="sliced" fillcenter="false" />
                <sprite color="[darkGrey]" type="sliced" />
                
