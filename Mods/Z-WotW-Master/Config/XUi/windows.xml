                        <label depth="3" pos="9, -6" text="{entrydescription}"  width="530" height="293" font_size="26" parse_actions="true" actions_default_format="( [e61a16]###[-] )" />
                    </rect>
                    
                    <rect name="ChallengeBox2" depth="1" pos="0,-383" width="553" height="336" visible="{# objectivecount == 2}" >
